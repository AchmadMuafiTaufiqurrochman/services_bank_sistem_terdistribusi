            "description": "Nomor rekening tujuan",
            "example": "0987654321"
          },
          "amount": {
            "type": "number",
            "exclusiveMinimum": 0,
            "title": "Amount",
            "description": "Jumlah yang akan ditransfer",
            "example": 100000
          },
          "currency_code": {
            "anyOf": [
              {
                "type": "string"
              },
              {
                "type": "null"
              }
            ],
            "title": "Currency Code",
            "description": "Kode mata uang",
            "default": "IDR",
            "example": "IDR"
