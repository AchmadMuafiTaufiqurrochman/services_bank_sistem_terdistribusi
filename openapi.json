          "description": {
            "anyOf": [
              {
                "type": "string"
              },
              {
                "type": "null"
              }
            ],
            "title": "Description",
            "description": "Deskripsi transaksi",
            "example": "Ini Deskripsi"
          },
          "PIN": {
            "type": "string",
            "title": "Pin",
            "description": "PIN",
            "example": 123456
          }
        },
